        <div id="chart-sensor-control">
            <div class="container">
                <div class="row">
                    <div class="col-12 col-sm-12 col-md-8 col-lg-8">
                        <div class="jumbotron chart-sensor">
                            <h3>Chart</h3>
                            <div class="line-chart-sensor">
                                <img class="line-chart-sensor-style" src="img-sensor/linered.png" alt="temp">
                                <p class="t-line">Temperature</p>
                                <img class="line-chart-sensor-style" src="img-sensor/linehum.png" alt="hum">
                                <p class="h-line">Humidity</p>
                                <img class="line-chart-sensor-style" src="img-sensor/linedust.png" alt="gas">
                                <p class="g-line">Gas</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-12 col-sm-12 col-md-4 col-lg-4">
                        <div class="jumbotron control-led">
                            <h3>Control</h3>
                            <img src="img/light-bulb.png" alt="light-on">
                            <input type="submit" id="on" value="ON">
                            <input type="submit" id="off" value="OFF">
                            <div class="slidecontainer">
                                <input type="range" min="0" max="255" value="0" class="slider" id="myRange">
                                <p class="pt-4 val">Value: <span id="demo"></span></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
